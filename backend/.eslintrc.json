{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier",
        "eslint:recommended"
    ],
    "plugins": [
        "import",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {},
    "settings": {
        "import/extensions": [
            ".js",
            ".jsx"
        ]
    }
}